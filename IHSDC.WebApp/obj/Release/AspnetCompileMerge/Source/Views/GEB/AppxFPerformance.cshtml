                @Html.DropDownListFor(m => m.ApprovedOnType, (new IHSDC.WebApp.Models.MasterModels()).LoadAircraftType(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true", @required = true })



            </div>
            <div class="col">

                <label><b> Approved On Date</b></label>
                @Html.TextBoxFor(m => m.ApprovedOnDate, new { @class = "form-control datepicker3", @required = true, @onchange = "Case(this)" })

            </div>
            <div class="col">
                <label><b> Approved Name Of Dte</b></label>
                @Html.TextBoxFor(m => m.ApprovedNameOfDte, new { @class = "form-control", @onchange = "Case(this)" })


            </div>
        </div>
        <div class="row">
            <div class="col">
                <label><b> Approved Rank Of Dte</b></label>
                @Html.DropDownListFor(m => m.ApprovedRankOfDte, (new IHSDC.WebApp.Models.MasterModels()).LoadRanks(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true" })



            </div>
            <div class="col">
                <label><b> Approved Appt Of Dte</b></label>
                @Html.DropDownListFor(m => m.ApprovedApptOfDte, (new IHSDC.WebApp.Models.MasterModels()).LoadAppt(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true" })



            </div>
            <div class="col" style="margin-top:2%">
                <input type="submit" value="Update" class="btn btn-success" />

            </div>

