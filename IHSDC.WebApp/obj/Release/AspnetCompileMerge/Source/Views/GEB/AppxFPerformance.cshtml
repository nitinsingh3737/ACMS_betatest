            </div>
            <div class="col">
                <label><b> Grd Sub Instr Ability Standard  </b></label>
                @Html.DropDownListFor(m => m.GrdSubInstrAbilityStandard, (new IHSDC.WebApp.Models.MasterModels()).LoadSpecialQualifiaction(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true", @required = true })



            </div>
            <div class="col">
                <label><b> Spl Knowledge Percentage  </b></label>

                @Html.DropDownListFor(m => m.SplKnowledgePercentage, (new IHSDC.WebApp.Models.MasterModels()).LoadPercentage(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true" })



            </div>
            <div class="col">
                <label><b> Spl Knowledge Standard</b></label>

                @Html.DropDownListFor(m => m.SplKnowledgeStandard, (new IHSDC.WebApp.Models.MasterModels()).LoadSpecialQualifiaction(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true" })





            </div>
        </div>
        <div class="row">
            <div class="col">
                <label><b> Overall Performance Percentage</b></label>
                @Html.DropDownListFor(m => m.OverallPerformancePercentage, (new IHSDC.WebApp.Models.MasterModels()).LoadPercentage(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true", @required = true })




            </div>
            <div class="col">
                <label><b> Overall Performance Standard</b></label>
                @Html.DropDownListFor(m => m.OverallPerformanceStandard, (new IHSDC.WebApp.Models.MasterModels()).LoadSpecialQualifiaction(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true", @required = true })


            </div>
            <div class="col">
                <label><b> Recommend Instr Cat</b></label>
                @Html.DropDownListFor(m => m.RecommendInstrCat, (new IHSDC.WebApp.Models.MasterModels()).LoadSpecialQualifiaction(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true", @required = true })



            </div>
            <div class="col">
                <label><b> Recommend OnType</b></label>
                @Html.DropDownListFor(m => m.RecommendOnType, (new IHSDC.WebApp.Models.MasterModels()).LoadAircraftType(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true", @required = true })




            </div>
        </div>
        <div class="row">
            <div class="col">
                <label><b> Recommend On Date</b></label>

                @Html.TextBoxFor(m => m.RecommendOnDate, new { @class = "form-control datepicker3", @required = true, @onchange = "Case(this)" })


            </div>
            <div class="col">
                <label><b> Name Of Examiner</b></label>
                @Html.TextBoxFor(m => m.NameOfExaminer, new { @class = "form-control", @required = true, @onchange = "Case(this)" })
            </div>
            <div class="col">
                <label><b> Rank Of Examiner</b></label>
                @Html.DropDownListFor(m => m.RankOfExaminer, (new IHSDC.WebApp.Models.MasterModels()).LoadRanks(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true"})




            </div>
            <div class="col">
                <label><b> Appt Of Examiner</b></label>
                @Html.DropDownListFor(m => m.ApptOfExaminer, (new IHSDC.WebApp.Models.MasterModels()).LoadAppt(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true" })





            </div>
        </div>

        <div class="row">
            <div class="col">
                <label><b> Approved Instr Cat</b></label>
                @Html.DropDownListFor(m => m.ApprovedInstrCat, (new IHSDC.WebApp.Models.MasterModels()).LoadSpecialQualifiaction(), "--Select--", new { @data_show_subtext = "true", @class = "select form-control", @data_live_search = "true", @required = true })



