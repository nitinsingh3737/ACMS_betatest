
@model IHSDC.WebApp.Models.TransationStrengthReturn
@using IHSDC.WebApp.Connection


@{
    ViewBag.Title = "Generate IAFZ Report ";
    Layout = "~/Views/Shared/_NewAA7Layout.cshtml";int sno = 0;
}
<div class="card-body">

    <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th rowspan="2" class="center">S/No</th>
                    <th rowspan="2" class="center">Auth Appt</th>
                    <th rowspan="2" class="center">Present Rank</th>
                    <th rowspan="2" class="center">Army No</th>
                    <th rowspan="2" class="center">Aviator Name</th>
                    <th rowspan="2" class="center">CDA(0) A/C No</th>
                    <th rowspan="2" class="center">Regt/Corps</th>
                    <th rowspan="2" class="center">Med Cat</th>

                    <th colspan="3" class="center">Date Of</th>
                    <th rowspan="2" class="center">Present Appt</th>
                    <th rowspan="2" class="center">SOS Off Offrs </th>
                    <th rowspan="2" class="center">Remarks</th>


                </tr>
                <tr>
                    <th class="center">TOS</th>
                    <th class="center">Birth</th>
                    <th class="center">Commission /Seniority</th>
                    
                </tr>
            </thead>
            <tbody id="tbody">
               
                    @foreach (var item in Model.ILTransationStrengthReturn)
                    {
                <tr>
                    <td>
                        @{ sno++; }
                        @sno
                    </td>
                    <td>@Html.DisplayFor(modelItem => item.AuthApp)</td>
                    <td>@Html.DisplayFor(modelItem => item.AviatorRank)</td>
                    <td>@Html.DisplayFor(modelItem => item.ANumber)</td>
                    <td>@Html.DisplayFor(modelItem => item.AviatorName)</td>

                    <td>@Html.TextBoxFor(modelItem => item.CDAcctNo, new { @class = "form-control" })</td>
                    <td>@Html.DisplayFor(modelItem => item.ArmServiceName)</td>
                    <td>@Html.DisplayFor(modelItem => item.MedicalCat)</td>
                    <td>@Html.DisplayFor(modelItem => item.TOS)</td>
                    <td>@Html.DisplayFor(modelItem => item.DateOfBirth)</td>
                    <td>@Html.DisplayFor(modelItem => item.DateOfSeniority)</td>
                    <td>@Html.TextBoxFor(modelItem => item.PresentAppt, new { @class = "form-control" })</td>
                    <td>@Html.TextBoxFor(modelItem => item.SoSOffrs, new { @class = "form-control" })</td>
                    <td>@Html.TextBoxFor(modelItem => item.Remarks,new { @class="form-control"})</td>


                </tr>
                    }
               
            </tbody>
        </table>
    </div>

</div>

