<script>

    $(".Redirectopage").off("click").click(function () {
        //alert($(this).closest("td").find(".PageUrl").html());
        var ss = $(this).closest("td").find(".PageUrl").html();
        /* alert($(this).closest("tr").find(".AviatorName").html());*/
        var param1 = { "AviatorIds": $(this).closest("tr").find(".Aviator_ID").html(), "AvitorName": $(this).closest("tr").find(".AviatorName").html() };
        $.ajax({
            url: '/AA7/SessionAviator',
            //data: "{'prefix': '" + request.term + "'}",
            data: JSON.stringify(param1),
            dataType: "json",
            type: "POST",
            contentType: "application/json; charset=utf-8",
            success: function (data) {
                if (data == 0)
                    window.location.replace(window.location.origin + "/AA7/" + ss);
                //alert(window.location.origin + "/AA7/" + ss);
                // alert(ss);
            }
        });
    });
</script>
