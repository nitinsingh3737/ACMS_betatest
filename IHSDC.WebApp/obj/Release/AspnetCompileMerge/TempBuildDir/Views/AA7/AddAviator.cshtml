        //    }
        //}

        $(document).ready(function () {

            if ($("#FlyingStatus").html() == 2) {
                $("#ReasonUnFitId").show();
                $("#ReasonUnFit").attr("required", true);
            } else {
                $("#ReasonUnFitId").hide();
                $("#ReasonUnFit").attr("required", false);
            }

        });
        $(document).ready(function () {

            $("#FlyingStatusId").on("change", function () {
                var selectedValue = $(this).val();
                if (selectedValue == 2) {
                    $("#ReasonUnFitId").show();
                    $("#ReasonUnFit").attr("required", true);
                } else {
                    $("#ReasonUnFitId").hide();
                    $("#ReasonUnFit").attr("required", false);
                }
            });


        });


    </script>



