    }

        table th {
            background-color: #e8e8e8 !important;
            print-color-adjust: exact;
            color: #333;
            font-weight: bold;
        }

        table th, table td {
            padding: 5px;
            border: 1px solid #000000;
        }

    @@media print {
        @@page {
            size: A4 portrait;
            margin-top: 10px;
            margin-bottom: 10px;
        }

        body::after {
            content: none !important;
        }

        .card-page {
            break-after: page;
        }

            .card-page:last-of-type {
                break-after: auto;
            }

        footer {
            position: fixed;
            bottom: 0;
            left: 0;
            right: 0;
            height: 10px;
            background-color: gray;
            color: white;
            text-align: center;
            padding: 10px;
        }

        header {
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            height: 10px;
            background-color: gray;
            color: white;
            text-align: center;
