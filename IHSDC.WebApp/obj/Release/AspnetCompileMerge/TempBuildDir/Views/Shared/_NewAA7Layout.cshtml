    <script>
        window.onscroll = function () { myFunction() };

        var header = document.getElementById("myHeader");
        var sticky = header.offsetTop;

        function myFunction() {
            if (window.pageYOffset > sticky) {
                header.classList.add("sticky");
            } else {
                header.classList.remove("sticky");
            }
        }
    </script>

    <script>
        const currentPage = window.location.pathname;

        // find the link that matches the current page URL and add the active class
        const links = document.querySelectorAll(".sidebar a");
        for (let i = 0; i < links.length; i++) {
            if (links[i].href === currentPage) {
                links[i].classList.add("active");
            }
        }
    </script>



    <script>
        $(document).ready(function () {
            //if (window.location.href.replace("http://localhost:8080/", ""))
            //alert(window.location.href.replace("http://localhost:8080/", ""));

            $('.aj1 ul li').find('a').each(function () {
                var text = $(this).attr("href");

                if (window.location.pathname.replace(window.location.origin, "") == text) {
                    var ss = (window.location.pathname.replace(window.location.origin, "")).replace("/", "").split("/")
                   /* alert(ss[0]);*/

                    $(this).addClass('active')
                  /*  $(this).addClass('active')*/
                }
                else {
                    $(this).removeClass("active");
                }
            });

            $('.aj1 ul li a').click(function () {
                $('li a').removeClass("active");
                $(this).addClass("active");

            });

        });
    </script>

